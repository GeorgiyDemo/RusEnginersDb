<Window x:Class="RusEnginersDb_CLIENT.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:RusEnginersDb_CLIENT"
        mc:Ignorable="d"
        Title="ChartWindow" Height="300" Width="300">
    <TabControl>
        <TabItem Header="Цены">
            <oxy:PlotView x:Name="CostPlot"/>
        </TabItem>
        <TabItem Header="Доставка">
            <oxy:PlotView x:Name="TimePlot"/>
        </TabItem>
    </TabControl>
</Window>
